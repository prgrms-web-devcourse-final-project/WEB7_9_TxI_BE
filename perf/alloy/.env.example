# Grafana Cloud Loki Configuration
# Copy this file to .env and fill in your actual values

# Loki Push URL (from Grafana Cloud)
# Example: https://logs-prod-030.grafana.net/loki/api/v1/push
LOKI_PUSH_URL=https://your-loki-url.grafana.net/loki/api/v1/push

# Grafana Cloud User ID (usually a number)
LOKI_USER_ID=your-user-id

# Grafana Cloud API Token (with Logs:Write and Metrics:Write permission)
LOKI_API_TOKEN=your-api-token

# Grafana Cloud Prometheus Remote Write URL
# Example: https://prometheus-prod-13-prod-us-east-0.grafana.net/api/prom/push
PROMETHEUS_REMOTE_WRITE_URL=https://your-prometheus-url.grafana.net/api/prom/push

PROM_USER_ID=your-user-id

PROM_API_TOKEN=your-api-token

# Spring Boot 주소 (Docker에서 호스트 접근)
# EC2 Linux: host.docker.internal:8080
SPRING_BOOT_HOST=host.docker.internal:8080

# Grafana Cloud Loki Configuration
# Copy this file to .env and fill in your actual values

# Loki Push URL (from Grafana Cloud)
# Example: https://logs-prod-030.grafana.net/loki/api/v1/push
LOKI_PUSH_URL=https://your-loki-url.grafana.net/loki/api/v1/push

# Grafana Cloud User ID (usually a number)
GRAFANA_USER_ID=your-user-id

# Grafana Cloud API Token (with Logs:Write permission)
GRAFANA_API_TOKEN=your-api-token
